{
	"name" : "pluf/workflow",
	"version" : "6.0.20",
	"description" : "Fast workflow manager.",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Mostafa Barmshory",
			"email" : "mostafa.barmshory@dpq.co.ir",
			"homepage" : "http://dpq.co.ir",
			"role" : "developer"
		}
	],
	"minimum-stability" : "stable",
	"require" : {
		"pluf/core" : "6.x"
	},
	"require-dev" : {
		"pluf/test" : "6.x"
	},
	"keywords" : [
		"pluf",
		"workflow"
	],
	"support" : {
		"email" : "info@pluf.ir",
		"issues" : "https://github.com/pluf/workflow/issues",
		"wiki" : "https://github.com/pluf/workflow/wiki",
		"source" : "https://github.com/pluf/workflow"
	},
	"homepage" : "http://pluf.ir",
	"autoload" : {
		"psr-4" : {
			"Pluf\\Workflow\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Pluf\\Test\\" : "tests",
			"Pluf\\NoteBook\\" : "example/NoteBook"
		}
	},
	"repositories~" : [{
			"type" : "path",
			"url" : "../core",
			"name" : "pluf/core",
			"options" : {
				"symlink" : true
			}
		}, {
			"type" : "path",
			"url" : "../test",
			"name" : "pluf/test",
			"options" : {
				"symlink" : true
			}
		}, {
			"type" : "path",
			"url" : "../http2",
			"name" : "pluf/http2",
			"options" : {
				"symlink" : true
			}
		}
	]
}